{
    "name": "umar/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel 9",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
    	{
            "name": "Umar Khalid",
            "email": "umark1142@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Ali Ahmad",
            "email": "alimughalktech@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": "5.* || 6.* || 7.* || 8.* || 9.* || 10.*"
    },
    "autoload": {
        "psr-4": {
          "Harimayco\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harimayco\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Harimayco\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
